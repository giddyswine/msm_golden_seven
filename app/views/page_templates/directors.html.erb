<h1>All Directors</h1>

<ul>
<% @director_array.each do |info| %>

  <div class="container">
    <div class="row">
        <div class="col-sm-3">
            <img src= "<%=Director.find(info).image_url %>">
            </div>
          
        <div class="col-sm-1">
              <p><%= Director.find(info).name %></p>
        </div>
        
        <div class="col-sm-6">
              <p><%= Director.find(info).bio %></p>
        </div>

        <div class="col-sm-2">
              <p><%= Director.find(info).dob %></p>
                        <div>
                <a href="/director/<%=Director.find(info).id%>/edit" class="btn btn-default">
                  Edit
                </a>
                </div>
                <a href="/delete_director/<%=Director.find(info).id%>" class="btn btn-default">
                  Delete
                </a>

        </div>
    </div>
